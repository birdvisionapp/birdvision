#widget-postion-1.col-sm-4
  .widget-postion-inner
    %span.delete-widget-button
      - @widget_id = @client_manager_dashboard.find_widget(@wid, current_admin_user)
      = link_to image_tag("remove_30-sign.jpg"), admin_client_managers_widget_path(@widget_id), :method => :delete, :class => "remove-widget-button", :confirm => "Are you sure. You want to remove #{@wid} widget?"
    = link_to traction_admin_client_managers_widgets_path, :id => "redemption-link", :class => "decoration-none" do
      .widget-header= 'Community Traction'
      #container{:style => "height:162px; width:570px; margin: 0 auto;"}
      
.hidden-values
  %span.commulative-count
    =@client_manager_dashboard.community_traction.first.to_json
  %span.non-commulative-count
    =@client_manager_dashboard.community_traction.last.to_json