%table.filters
  %tr
    - if is_super_admin?
      %td
        = f.label :msp_id_eq, "MSP"
        = f.collection_select :msp_id_eq, Msp.accessible_by(current_ability).select_options, :id, :name, {:include_blank => t(:all)}, :class => 'msp-selector-init'
    %td
      = f.label :client_name_cont, "Client Name"
      = f.text_field :client_name_cont, :class => 'input-medium'
    %td
      = f.label :contact_name_cont, "Contact Name"
      = f.text_field :contact_name_cont, :class => 'input-medium'
    %td
      = f.label :contact_email_cont, "Contact Email"
      = f.text_field :contact_email_cont, :class => 'input-medium'
